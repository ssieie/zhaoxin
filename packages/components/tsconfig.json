{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": [
        "env.d.ts",
        "types.d.ts",
        "src/**/*",
        "src/**/*.vue"
    ],
    "compilerOptions": {
        "composite": true,
        "moduleResolution": "node",
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ]
        }
    }
}